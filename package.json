{
  "name": "live-page",
  "displayName": "Live Page",
  "description": "",
  "icon": "images/cat.jpg",
  "version": "0.0.8",
  "publisher": "LivePage",
  "engines": {
    "vscode": "^1.76.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "when": "resourceScheme == 'file'",
          "command": "live-page.open",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "live-page.open",
        "title": "LivePage: Open a live page in your browser"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "opn": "^6.0.0"
  },
  "license": "MIT"
}
